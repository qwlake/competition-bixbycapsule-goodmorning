structure (Timer) {
  property (timerTime) {
    type (time.DateTime)
    min (Required) max (One)
  }
  property (sleepCycle) {
    type (SleepCycle)
    min (Required) max (One)
  }
  property (imageUrl) {
    type (ImageUrl)
    min (Required) max (One)
  }
  property (reason) {
    type (Reason)
    min (Optional) max (One)
  }
  property (hasReason) {
    type (core.Boolean)
    min (Required) max (One)
  }
}