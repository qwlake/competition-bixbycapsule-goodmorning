structure (Timer) {
  property (timerTime) {
    type (time.DateTime)
    min (Required) max (One)
  }
  property (stringTime) {
    type (StringTime)
    min (Optional) max (One)
  }
  property (reason) {
    type (Reason)
    min (Optional) max (One)
  }
  property (hasReason) {
    type (core.Boolean)
    min (Required) max (One)
  }
}