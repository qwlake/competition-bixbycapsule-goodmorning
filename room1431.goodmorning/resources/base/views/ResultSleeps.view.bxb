result-view {
  match: ResultSleeps (this)

  message {
    template ("주무실 시간을 선택해 주세요.") 
  }

  render {
    list-of (this.resultSleep) {
      where-each (item) {
        layout-macro (TimeCard) {
          param (timeCard) {
            expression (item)
          }
        }
      }
    }
  }
}